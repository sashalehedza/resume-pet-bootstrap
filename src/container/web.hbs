{{#with web}}
<h2>Web Languages</h2>
<ul>
  {{#each languages}}
  <li>
    <h3>{{name}}</h3>
    <p>{{version}}</p>
    <p>{{description}}</p>

    {{#if elements}}
    <h4>Elements:</h4>
    <ul>
      {{#each elements}}
      <li>{{name}}</li>
      <p>{{description}}</p>
      {{#each attributes}}
      <p>{{name}}</p>
      <p>{{description}}</p>
      {{/each}}
      {{/each}}
    </ul>
    {{/if}}

    {{#if properties}}
    <h4>Properties:</h4>
    <ul>
      {{#each properties}}

      <li>{{name}}</li>
      <p>{{description}}</p>
      {{#each values}}
      <p>{{value}}</p>
      <p>{{description}}</p>
      {{/each}}
      {{/each}}
    </ul>
    {{/if}}

    {{#if functions}}
    <h4>Functions:</h4>
    <ul>
      {{#each functions}}

      <li>{{name}}</li>
      <p>{{description}}</p>
      {{#each parameters}}
      <p>{{name}}</p>
      <p>{{type}}</p>
      <p>{{description}}</p>
      {{/each}}
      {{/each}}
    </ul>
    {{/if}}

    <a href="/program">{{name}} Program</a>

  </li>
  {{/each}}
</ul>
{{/with}}